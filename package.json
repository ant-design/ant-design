    "rc-form": "~1.3.0",
    "rc-input-number": "~3.4.4",
    "rc-menu": "~5.0.9",
    "rc-notification": "~1.4.0",
    "rc-pagination": "~1.8.7",
    "rc-progress": "~2.1.0",
    "rc-radio": "~2.0.0",
    "rc-rate": "~2.1.0",
    "rc-select": "~6.8.0",
    "rc-slider": "~7.0.0",
    "rc-steps": "~2.5.0",
    "rc-switch": "~1.4.2",
    "rc-table": "~5.2.13",
    "rc-tabs": "~7.5.0",
    "rc-time-picker": "~2.4.0",
    "rc-tooltip": "~3.4.2",
    "rc-tree": "~1.5.0",
    "rc-tree-select": "~1.9.0",
    "rc-upload": "~2.3.0",
    "rc-util": "^4.0.1",
    "react-lazy-load": "^3.0.10",
    "react-slick": "~0.14.2",
