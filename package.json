    "rc-slider": "~10.0.0-alpha.4",
    "rc-steps": "~4.1.0",
    "rc-switch": "~3.2.0",
    "rc-table": "~7.23.0",
    "rc-tabs": "~11.10.0",
    "rc-textarea": "~0.3.0",
    "rc-tooltip": "~5.1.1",
    "rc-tree": "~5.4.3",
