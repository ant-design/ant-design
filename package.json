  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "size-limit": [
    {
      "path": "./dist/antd.min.js",
      "limit": "385 KiB"
    },
    {
      "path": "./dist/antd-with-locales.min.js",
      "limit": "500 KiB"
    }
  ],
  "tnpm": {
    "mode": "npm"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "rome format --write",
    "*.{json,less,md}": "prettier --ignore-unknown --write"
  }
