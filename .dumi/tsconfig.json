{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "baseUrl": "../",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "antd": ["components/index.tsx"],
      "antd/es/*": ["components/*"],
      "dumi/theme/*": [".dumi/theme/*"]
    }
  },
  "include": ["./**/*", "../site/theme/template/Content"]
}
